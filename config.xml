<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.coachseek" version="0.1.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Coachseek</name>
    <description>
       A single place online to manage your sports coaching business. That's Coachseek.
    </description>
    <author email="hello@coachseek.com" href="http://www.coachseek.com">
        Coachseek
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="22" />
    <plugin name="cordova-plugin-facebook4" spec="~1.3.0-0">
        <variable name="APP_ID" value="172144976459129" />
        <variable name="APP_NAME" value="coachseek" />
    </plugin>
</widget>
